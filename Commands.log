# Connect you local repo to remote repo using this below command 
git remote add origin [here add your gitHub web url]

#Command to push your local repo to remote repo
git push origin master

#Command that get used to rename files
git mv 

# Command to revert git commit
git revert <commit_id>

# 
git fetch origin 
#or 
git fetch --all

git diff-tree -r <commit-id>
git version
git help

# Commands to check details
git config --list
git config user.name
git config user.email
# OR
git config --get user.name
git config --get user.email
# OR 
git config --global user.name
git config --global user.email 

# Commands to set user details
git config --global user.name "USER_NAME"
git config --global user.email "username@gmail.com"
git config --global user.password "1234321"

# Commands to unset user details
git config --global --unset user.name
git config --global --unset user.email
git config --global --unset credential.helper

# to initial git repository 
git init
git status
git add .
git commit -m "Add message"
